{
  "info": {
    "name": "Nomino Pro API Collection",
    "description": "Complete API collection for Nomino Pro Food Delivery Management System",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{authToken}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5000",
      "type": "string"
    },
    {
      "key": "authToken",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"test_user\",\n  \"email\": \"test@example.com\",\n  \"password\": \"password123\",\n  \"firstName\": \"Test\",\n  \"lastName\": \"User\",\n  \"phone\": \"9876543210\",\n  \"role\": \"delivery_partner\",\n  \"vehicleType\": \"scooter\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "register"]
            }
          }
        },
        {
          "name": "Login User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('authToken', response.token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@nominopro.com\",\n  \"password\": \"admin123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "login"]
            }
          }
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/auth/profile",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "profile"]
            }
          }
        }
      ]
    },
    {
      "name": "Orders",
      "item": [
        {
          "name": "Get All Orders",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/orders?page=1&limit=10&status=PENDING",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                },
                {
                  "key": "status",
                  "value": "PENDING"
                }
              ]
            }
          }
        },
        {
          "name": "Create Order",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"customerName\": \"John Doe\",\n  \"customerPhone\": \"9876543210\",\n  \"customerAddress\": {\n    \"street\": \"123 Main Street\",\n    \"city\": \"Mumbai\",\n    \"pincode\": \"400001\"\n  },\n  \"items\": [\n    {\n      \"name\": \"Butter Chicken\",\n      \"quantity\": 2,\n      \"price\": 320,\n      \"category\": \"main_course\"\n    },\n    {\n      \"name\": \"Naan\",\n      \"quantity\": 4,\n      \"price\": 60,\n      \"category\": \"side_dish\"\n    }\n  ],\n  \"totalAmount\": 380,\n  \"prepTime\": 25,\n  \"estimatedDeliveryTime\": 30\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/orders",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders"]
            }
          }
        },
        {
          "name": "Get Order by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/orders/:orderId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders", ":orderId"],
              "variable": [
                {
                  "key": "orderId",
                  "value": "ORDER_ID_HERE"
                }
              ]
            }
          }
        },
        {
          "name": "Update Order Status",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"PREP\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/orders/:orderId/status",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders", ":orderId", "status"],
              "variable": [
                {
                  "key": "orderId",
                  "value": "ORDER_ID_HERE"
                }
              ]
            }
          }
        },
        {
          "name": "Assign Delivery Partner",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"deliveryPartnerId\": \"PARTNER_ID_HERE\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/orders/:orderId/assign",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders", ":orderId", "assign"],
              "variable": [
                {
                  "key": "orderId",
                  "value": "ORDER_ID_HERE"
                }
              ]
            }
          }
        },
        {
          "name": "Rate Order",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"deliveryService\": 4.5,\n  \"foodQuality\": 4.2,\n  \"overallExperience\": 4.3,\n  \"feedback\": \"Great service, food arrived hot!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/orders/:orderId/rating",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders", ":orderId", "rating"],
              "variable": [
                {
                  "key": "orderId",
                  "value": "ORDER_ID_HERE"
                }
              ]
            }
          }
        },
        {
          "name": "Get Order Statistics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/orders/stats?period=today",
              "host": ["{{baseUrl}}"],
              "path": ["api", "orders", "stats"],
              "query": [
                {
                  "key": "period",
                  "value": "today"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Delivery Partners",
      "item": [
        {
          "name": "Get All Partners",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/partners?isAvailable=true&vehicleType=scooter&sortBy=rating&sortOrder=desc",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners"],
              "query": [
                {
                  "key": "isAvailable",
                  "value": "true"
                },
                {
                  "key": "vehicleType",
                  "value": "scooter"
                },
                {
                  "key": "sortBy",
                  "value": "rating"
                },
                {
                  "key": "sortOrder",
                  "value": "desc"
                }
              ]
            }
          }
        },
        {
          "name": "Get Available Partners",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/partners/available",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners", "available"]
            }
          }
        },
        {
          "name": "Get Partner Orders",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/partners/my/orders?status=PICKED&page=1&limit=20",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners", "my", "orders"],
              "query": [
                {
                  "key": "status",
                  "value": "PICKED"
                },
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "20"
                }
              ]
            }
          }
        },
        {
          "name": "Get Partner Active Orders",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/partners/my/active-orders",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners", "my", "active-orders"]
            }
          }
        },
        {
          "name": "Get Partner Statistics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/partners/my/stats?period=week",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners", "my", "stats"],
              "query": [
                {
                  "key": "period",
                  "value": "week"
                }
              ]
            }
          }
        },
        {
          "name": "Update Availability",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"isAvailable\": true\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/partners/availability",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners", "availability"]
            }
          }
        },
        {
          "name": "Get Partner Details",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/api/partners/:partnerId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "partners", ":partnerId"],
              "variable": [
                {
                  "key": "partnerId",
                  "value": "PARTNER_ID_HERE"
                }
              ]
            }
          }
        }
      ]
    }
  ]
} 